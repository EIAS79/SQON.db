{
  "name": "nuvira-parser",
  "version": "1.0.0",
  "description": "Nuvira Database. New Database format (Readable & Easy to use), (Inbuilt Schema & constraints & rules & relations).",
  "main": "./dist/cjs/Nuvira.js",
  "module": "./dist/esm/Nuvira.js",
  "types": "./dist/esm/Nuvira.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/Nuvira.js",
      "import": "./dist/esm/Nuvira.js"
    }
  },
  "author": "zenith-79",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:esm": "tsc --build tsconfig.esm.json --force",
    "build:cjs": "tsc --build tsconfig.cjs.json --force",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "watch": "tsc -w",
    "start": "node ./dist/cjs/Nuvira.js",
    "dev": "ts-node ./lib/Nuvira.ts",
    "publish:prep": "npm run build",
    "publish": "npm run publish:prep && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EIAS79/Nuvira.git"
  },
  "bugs": {
    "url": "https://github.com/EIAS79/Nuvira/issues"
  },
  "files": [
    "lib",
    "dist",
    "example",
    "changelog.md"
  ],
  "keywords": [
    "Nuvira",
    "custom database",
    "custom format database",
    "parser",
    "Nuvira-parser",
    "Nuvira parser",
    "JSON",
    "SQL",
    "database",
    "Nuvira",
    "data conversion",
    "format conversion",
    "json to Nuvira",
    "Nuvira to json",
    "sql to Nuvira",
    "Nuvira to sql",
    "yaml to Nuvira",
    "Nuvira to yaml",
    "csv to Nuvira",
    "Nuvira to csv",
    "xml to Nuvira",
    "Nuvira to xml",
    "data transformation",
    "data serialization",
    "cross-format conversion",
    "data interchange",
    "relational database",
    "non-relational database",
    "NoSQL",
    "SQL to NoSQL",
    "NoSQL to SQL",
    "relational data model",
    "non-relational data model",
    "fast",
    "reliable"
  ],
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "npm": "^11.0.0"
  }
}
